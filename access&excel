Sub test()
Dim conn As New ADODB.Connection
conn.Open "Provider=Microsoft.ACE.OLEDB.12.0;Data Source=D:\Excel 2010VBA\王佩丰\王佩丰 VBA 课件\第十九课\Database11.accdb"
